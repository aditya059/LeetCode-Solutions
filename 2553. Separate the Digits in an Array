class Solution {
public:
    vector<int> separateDigits(vector<int>& nums) {
        vector<int> ans;
        for(int i = nums.size() - 1; i >= 0; i--) {
            for(int j = nums[i]; j > 0; j /= 10) {
                ans.push_back(j % 10);
            }
        }
        reverse(ans.begin(), ans.end());
        return ans;
    }
};
