class Solution {
public:
    vector<int> createTargetArray(vector<int>& nums, vector<int>& index) {
        int n = nums.size();
        vector<int> ans;
        for(int i = 0; i < n; i++) {
            if(index[i] == nums.size()) {
                ans.push_back(nums[i]);
            } else {
                ans.push_back(0);
                for(int j = ans.size() - 1; j > index[i]; j--) {
                    ans[j] = ans[j - 1];
                }
                ans[index[i]] = nums[i];
            }
        }
        return ans;
    }
};
