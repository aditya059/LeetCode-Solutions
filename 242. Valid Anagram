class Solution {
public:
    bool isAnagram(string &s, string &t) {
        if(s.size() != t.size()) return false;
        int freq[128] = {0};
        for(int i = 0; i < s.length(); i++) {
            freq[s[i]]++;
            freq[t[i]]--;
        }
        for(int i = 0; i < 128; i++) {
            if(freq[i] != 0) 
                return false;
        }
        return true;
    }
};
