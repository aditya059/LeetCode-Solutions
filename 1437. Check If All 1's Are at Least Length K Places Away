class Solution {
public:
    bool kLengthApart(vector<int>& nums, int k) {
        int prevOne = -(k + 1);
        for(int i = 0; i < nums.size(); i++) {
            if(nums[i] == 1) {
                if(i - prevOne <= k) return false;
                prevOne = i;
            }
        }
        return true;
    }
};
