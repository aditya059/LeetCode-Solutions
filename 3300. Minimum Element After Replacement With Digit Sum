class Solution {
public:
    int minElement(vector<int>& nums) {
        int ans = INT_MAX;
        for(int i = 0; i < nums.size(); i++) {
            int sum = 0;
            for(int j = nums[i]; j > 0; j /= 10) {
                sum += j % 10;
            }
            ans = min(ans, sum);
        }
        return ans;
    }
};
