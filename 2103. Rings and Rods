class Solution {
public:
    int countPoints(string rings) {
        int n = rings.size(), ans = 0;
        vector<int> mask(10);
        for(int i = 0; i < n; i += 2) {
            int pos = rings[i] == 'R'? 0: rings[i] == 'G'? 1: 2;
            mask[rings[i + 1] - '0'] |= (1 << pos); 
        }
        for(int i = 0; i < 10; i++) {
            ans += mask[i] == 7;
        }
        return ans;
    }
};
