class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        set<int> Set;
        set<int> Set1;
        vector<int> ans;
        for(int i = 0; i < nums1.size(); i++)
            Set.insert(nums1[i]);
        for(int i = 0; i < nums2.size(); i++) {
            if(Set.find(nums2[i]) != Set.end() && Set1.find(nums2[i]) == Set1.end()) {
                ans.push_back(nums2[i]);
                Set1.insert(nums2[i]);
            }
        }
        return ans;
    }
};
