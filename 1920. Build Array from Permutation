class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        vector<int> ans(nums.size());
        for(int i = 0; i < nums.size(); i++) {
            ans[i] = nums[nums[i]];
        }
        return ans;
    }
};

OR

class Solution {
public:
    vector<int> buildArray(vector<int>& nums) {
        for(int i = 0; i < nums.size(); i++) {
            nums[i] = (nums[nums[i]] % 1000) * 1000 + nums[i];
        }
        for(int i = 0; i < nums.size(); i++) {
            nums[i] /= 1000; 
        }
        return nums;
    }
};
