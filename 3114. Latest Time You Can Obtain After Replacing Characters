class Solution {
public:
    string findLatestTime(string s) {
        if(s[4] == '?') s[4] = '9';
        if(s[3] == '?') s[3] = '5';
        if(s[0] == '?') {
            s[0] = '0' + !(s[1] >= '2' && s[1] <= '9');
        }
        if(s[1] == '?') {
            s[1] = s[0] == '0'? '9': '1';
        }
        return s;
    }
};
