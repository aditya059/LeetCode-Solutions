class Solution {
public:
    int maxSubarrayLength(vector<int>& nums, int k) {
        int ans = 0;
        unordered_map<int, int> Map;
        for(int start = 0, end = 0; end < nums.size(); end++) {
            Map[nums[end]]++;
            while(Map[nums[end]] > k) {
                Map[nums[start++]]--;
            } 
            ans = max(ans, end - start + 1);
        }
        return ans;
    }
};
