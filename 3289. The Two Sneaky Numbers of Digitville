class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        int n = nums.size();
        vector<int> freq(n - 2), ans;
        for(int i = 0; i < nums.size(); i++) {
            if(freq[nums[i]]) {
                ans.push_back(nums[i]);
            }
            freq[nums[i]]++;
        }
        return ans;
    }
};
