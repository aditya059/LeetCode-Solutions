class Solution {
public:
    int countEven(int num) {
        int count = 0;
        for(int i = 1; i <= num; i++) {
            int sum = 0;
            for(int j = i; j > 0; j /= 10) {
                sum += j % 10;
            }
            if(sum % 2 == 0)
                count++;
        }
        return count;
    }
};
